{
    "name": "islandora/crayfish",
    "description": "RESTful services container",
    "support": {
        "issues": "https://github.com/islandora-claw/crayfish/issues",
        "irc": "irc://irc.freenode.net/islandora"
    },
    "repositories": [
      {
                "type": "path",
                "url": "ResourceService"
      },
      {
                "type": "path",
                "url": "CollectionService"
      },
      {
                "type": "path",
                "url": "TransactionService"
      }
    ],
    "require": {
        "islandora/chullo": "^0.0",
        "islandora/resource-service" : "^0.0",
        "islandora/collection-service" : "^0.0",
        "silex/silex": "^1.3",
        "symfony/config": "^3.0",
        "twig/twig": "^1.23",
        "symfony/yaml": "^3.0",
        "ramsey/uuid": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.0@dev"
    },
    "autoload": {
        "psr-4": {"Islandora\\Crayfish\\": "src/"}
    },
    "minimum-stability" : "dev",
    "license": "MIT",
